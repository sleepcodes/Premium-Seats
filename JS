function isChecked() {
  let premium = document.getElementById("Premium").checked;
  let regular = document.getElementById("Regular").checked;  
  if (premium) {
    console.log("You have selected the Premium option!");
  }
  else if (!premium && !regular) {
    console.log("You must pick an option!");
  }
  else {
    console.log("You have selected the Regular option!");
  }
}
  
